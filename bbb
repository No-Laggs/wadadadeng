-- Loading Script Text
local t = Instance.new("TextLabel", f)
t.Size = UDim2.new(1, 0, 0.09, 0)
t.Position = UDim2.new(0, 0, 0.53, 0)
t.BackgroundTransparency = 1
t.TextColor3 = Color3.fromRGB(255, 255, 255)
t.Font = Enum.Font.GothamSemibold
t.TextScaled = true
t.Text = "üîÑ Loading Script"

local a = true
task.spawn(function()
	local d = 0
	while a and t.Parent do
		t.Text = "üîÑ Loading Script" .. string.rep(".", d % 4)
		d += 1
		wait(0.5)
	end
end)

-- Progress Bar
local pb = Instance.new("Frame", f)
pb.Size = UDim2.new(0.85, 0, 0.18, 0)
